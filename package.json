{
  "private": true,
  "name": "clearpoint-todolist",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "frontend": "yarn workspace todolist.api dev",
    "backend": "yarn workspace todolist.app dev",
    "dev": "concurrently --kill-others-on-fail 'yarn backend'  'yarn frontend'"
  },
  "dependencies": {
    "concurrently": "^7.0.0"
  }
}
